{
  "name": "flavio",
  "version": "2.5.0-snapshot.0",
  "preferGlobal": true,
  "analyze": false,
  "description": "Really simple package manager for your own private packages hosted on any git or svn server. No registry or server hosting required. Can be used in conjunction with bower/npm",
  "main": "index.js",
  "bin": {
    "flavio": "./bin/flavio"
  },
  "scripts": {
    "start": "node cli.js",
    "test": "npm run compile && mocha --require @babel/register",
    "lint": "eslint src test testutil",
    "lintfix": "eslint src test testutil --fix",
    "repo": "node -r @babel/register testutil/createRepo_cli.js",
    "compile": "./node_modules/.bin/babel src --out-dir lib",
    "dev": "./node_modules/.bin/babel -w src --out-dir lib",
    "prepublish": "npm run compile"
  },
  "author": "Pete Ward <peteward44@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "chalk": "1.1.3",
    "easy-table": "1.1.0",
    "fs-extra": "0.30.0",
    "inquirer": "1.2.3",
    "lodash": "4.17.4",
    "semver": "5.4.1",
    "uuid": "3.0.1",
    "yargs": "6.6.0"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.11.0",
    "mocha": "^3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peteward44/flavio.git"
  },
  "keywords": [
    "cli",
    "bower",
    "install",
    "modules",
    "package manager"
  ],
  "files": [
    "lib",
    "index.js",
    "cli.js",
    "bin",
    "npm-shrinkwrap.json",
    "README.md"
  ]
}