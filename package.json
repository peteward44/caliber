{
  "name": "flavio",
  "version": "4.4.0-snapshot.0",
  "preferGlobal": true,
  "analyze": false,
  "description": "Really simple package manager for your own private packages hosted on any git or svn server. No registry or server hosting required. Can be used in conjunction with bower/npm",
  "main": "index.js",
  "bin": {
    "flavio": "./bin/flavio"
  },
  "scripts": {
    "start": "webpack --watch --progress --mode development",
    "test": "mocha --require @babel/register",
    "lint": "eslint src test testutil",
    "lintfix": "eslint src test testutil --fix",
    "repo": "node -r @babel/register testutil/createRepo_cli.js",
    "prepublish": "npm run build",
    "build": "webpack --mode production"
  },
  "author": "Pete Ward <peteward44@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "chalk": "4.1.0",
    "core-js": "3.8.3",
    "easy-table": "1.1.1",
    "fs-extra": "9.1.0",
    "inquirer": "7.3.3",
    "lodash": "4.17.20",
    "moment": "2.29.1",
    "semver": "7.3.4",
    "uuid": "8.3.2",
    "yargs": "16.2.0"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.0.0",
    "@webpack-cli/init": "^0.3.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "chai": "^4.2.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.11.0",
    "html-webpack-plugin": "^5.1.0",
    "mocha": "^8.3.0",
    "webpack": "^5.21.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peteward44/flavio.git"
  },
  "keywords": [
    "cli",
    "bower",
    "install",
    "modules",
    "package manager"
  ],
  "files": [
    "src",
    "index.js",
    "dist/flavio.min.js",
    "bin",
    "yarn.lock",
    "README.md"
  ]
}
